<?xml version="1.0" encoding="UTF-8"?>
<ProductMetaData>
    <ProductID>[Product_Unique_Identifier]</ProductID>
    <ProductName>[Product_Name_or_FileName_Stem]</ProductName>
    <ProductLevel>[Processing_Level e.g., L1A, L1B, L2A, SC]</ProductLevel>
    <ProductFormat>[Data_Format e.g., GEOTIFF, DIMAP, SAFE]</ProductFormat>
    <ProduceTime>[YYYY-MM-DDTHH:MM:SSZ]</ProduceTime>
    <ProcessingSource>[Identifier_of_Processing_Center_or_System]</ProcessingSource> 
    <ProcessingSoftwareVersion>[Version_String]</ProcessingSoftwareVersion> 
    <DDSFlag>[TRUE/FALSE]</DDSFlag> <SatelliteID>[Satellite_Identifier e.g., GF2, ZY3-2, GF7-1]</SatelliteID>
    <SensorID>[Sensor_Identifier e.g., PMS2, NAD, AHSI, VNIC]</SensorID>
    <SensorMode>[Operating_Mode e.g., NADIR, STEREO, SCAN]</SensorMode>
    <InstrumentMode>[Instrument_Specific_Mode_Code e.g., 0 for VIMI]</InstrumentMode> 

    <ReceiveStationID>[Ground_Station_Identifier]</ReceiveStationID>
    <ReceiveTime>[YYYY-MM-DDTHH:MM:SSZ]</ReceiveTime>
	<TaskType>[Tasking_Type e.g., STANDARD, URGENT, NORMAL]</TaskType>
    <StripID>[Unique_Identifier_for_Data_Strip]</StripID>
    <SceneID>[Unique_Identifier_for_Scene]</SceneID>

    <OrbitID>[Orbit_Number]</OrbitID>
    <OrbitDirection>[Ascending/Descending]</OrbitDirection>  
    <OrbitDataSource>[Source_of_Orbit_Data e.g., GPS, PREDICTED]</OrbitDataSource> 
    <AttitudeDataSource>[Source_of_Attitude_Data e.g., STAR, DMA, GYRO, PATT]</AttitudeDataSource> 
    <AuxDataQuality>[Quality_Flag_for_Aux_Data e.g., STABLE, UNSTABLE, NOMINAL]</AuxDataQuality> 
    <RollSatelliteAngle unit="deg">[Satellite_Roll_Angle_at_Center]</RollSatelliteAngle>
    <PitchSatelliteAngle unit="deg">[Satellite_Pitch_Angle_at_Center]</PitchSatelliteAngle>
    <YawSatelliteAngle unit="deg">[Satellite_Yaw_Angle_at_Center]</YawSatelliteAngle>
    <SwingAngle unit="deg">[Across_Track_Swing_Angle]</SwingAngle> 
	<TimeType>[Timezone_Reference e.g., UTC, BJ]</TimeType>
    <StartTime>[YYYY-MM-DDTHH:MM:SS.sssZ]</StartTime> 
	<EndTime>[YYYY-MM-DDTHH:MM:SS.sssZ]</EndTime> 
	<CenterTime>[YYYY-MM-DDTHH:MM:SS.sssZ]</CenterTime>

    <ScenePath>[WRS_Path_Number, if applicable]</ScenePath>
    <SceneRow>[WRS_Row_Number, if applicable]</SceneRow>
    <SceneCount>[Number_of_Scenes_in_Product, if applicable]</SceneCount>
    <SceneShift>[Shift_Relative_to_WRS, if applicable]</SceneShift>
    <RegionName>[Geographic_Region_Identifier]</RegionName>
    <GeographicBoundary>
        <CenterPoint>
            <Latitude unit="deg">[Center_Latitude]</Latitude>
            <Longitude unit="deg">[Center_Longitude]</Longitude>
            <Altitude unit="m">[Center_Altitude_from_DEM/Average]</Altitude>
        </CenterPoint>
        <TopLeftPoint>
            <Latitude unit="deg">[Top_Left_Latitude]</Latitude>
            <Longitude unit="deg">[Top_Left_Longitude]</Longitude>
            <Altitude unit="m">[Top_Left_Altitude]</Altitude>
        </TopLeftPoint>
        <TopRightPoint>
            <Latitude unit="deg">[Top_Right_Latitude]</Latitude>
            <Longitude unit="deg">[Top_Right_Longitude]</Longitude>
            <Altitude unit="m">[Top_Right_Altitude]</Altitude>
        </TopRightPoint>
        <BottomRightPoint>
            <Latitude unit="deg">[Bottom_Right_Latitude]</Latitude>
            <Longitude unit="deg">[Bottom_Right_Longitude]</Longitude>
            <Altitude unit="m">[Bottom_Right_Altitude]</Altitude>
        </BottomRightPoint>
        <BottomLeftPoint>
            <Latitude unit="deg">[Bottom_Left_Latitude]</Latitude>
            <Longitude unit="deg">[Bottom_Left_Longitude]</Longitude>
            <Altitude unit="m">[Bottom_Left_Altitude]</Altitude>
        </BottomLeftPoint>
    </GeographicBoundary>
    <MapProjectionInfo>
        <ProjectionName>[Projection_System e.g., UTM, Geographic, Lambert]</ProjectionName>
        <ProjectionZone>[Zone_Number, if applicable]</ProjectionZone>
        <EarthEllipsoid>[Ellipsoid_Name e.g., WGS84]</EarthEllipsoid>
        <ProjectedCorners>
             <TopLeftEasting unit="m">[Top_Left_Easting/X_Coordinate]</TopLeftEasting>
             <TopLeftNorthing unit="m">[Top_Left_Northing/Y_Coordinate]</TopLeftNorthing>
             <TopRightEasting unit="m">[Top_Right_Easting/X_Coordinate]</TopRightEasting>
             <TopRightNorthing unit="m">[Top_Right_Northing/Y_Coordinate]</TopRightNorthing>
             <BottomRightEasting unit="m">[Bottom_Right_Easting/X_Coordinate]</BottomRightEasting>
             <BottomRightNorthing unit="m">[Bottom_Right_Northing/Y_Coordinate]</BottomRightNorthing>
             <BottomLeftEasting unit="m">[Bottom_Left_Easting/X_Coordinate]</BottomLeftEasting>
             <BottomLeftNorthing unit="m">[Bottom_Left_Northing/Y_Coordinate]</BottomLeftNorthing>
        </ProjectedCorners>
    </MapProjectionInfo>

    <ImageWidthInPixels>[Total_Width_in_Pixels]</ImageWidthInPixels>
    <ImageHeightInPixels>[Total_Height_in_Pixels]</ImageHeightInPixels>
    <ImageWidthInMeters unit="m">[Approximate_Ground_Width]</ImageWidthInMeters>
    <ImageHeightInMeters unit="m">[Approximate_Ground_Height]</ImageHeightInMeters>
    <PixelDataType>[Data_Type e.g., uint16, uint12, float32]</PixelDataType>
    <PixelInvalidValue>[Value_Representing_NoData_or_Invalid_Pixels]</PixelInvalidValue>
    <AverageGSD unit="m">[Average_Ground_Sample_Distance]</AverageGSD>
    <LineGSD unit="m">[Ground_Sample_Distance_in_Line_Direction, if different]</LineGSD>
    <SampleGSD unit="m">[Ground_Sample_Distance_in_Sample_Direction, if different]</SampleGSD>
    <BandsCount>[Number_of_Bands]</BandsCount>
    <BandDescription>
        <Band>
            <BandID>[Band_Identifier e.g., 1, B1, PAN, Blue, C01]</BandID>
            <BandName>[Common_Name e.g., Blue, Green, Red, NIR1, PAN]</BandName>
            <Bandwidth unit="nm or um">[Full_Width_at_Half_Maximum_or_Bandwidth]</Bandwidth>
            <SpectralRangeStart unit="nm or um">[Start_Wavelength]</SpectralRangeStart>
            <SpectralRangeEnd unit="nm or um">[End_Wavelength]</SpectralRangeEnd>
            <RadiometricGain unit="[Units_per_DN]">[Gain_Value]</RadiometricGain>
            <RadiometricOffset unit="[Units_of_Radiance/Reflectance]">[Offset_Value]</RadiometricOffset>
            <AbsoluteCalibrationCoefficient unit="[Units_per_DN]">[Absolute_Coefficient]</AbsoluteCalibrationCoefficient>
            <RadiometricFormula>[Formula_Description e.g., L=Gain*DN+Offset]</RadiometricFormula>
            <MinSpectralValue>[Min_DN_Value_or_Radiance]</MinSpectralValue>
            <MaxSpectralValue>[Max_DN_Value_or_Radiance]</MaxSpectralValue>
        </Band>
        </BandDescription>
    <FusionMethod>[Method_if_Pansharpened_or_Fused e.g., PAN, BGRN, PCA]</FusionMethod>

    <SunAzimuth unit="deg">[Sun_Azimuth_Angle]</SunAzimuth>
    <SunZenith unit="deg">[Sun_Zenith_Angle]</SunZenith>
    <SunAltitude unit="deg">[Sun_Altitude/Elevation_Angle (90-Zenith)]</SunAltitude>
    <SatelliteAzimuth unit="deg">[Satellite_Azimuth_Angle]</SatelliteAzimuth>
    <SatelliteZenith unit="deg">[Satellite_Zenith_Angle]</SatelliteZenith>
    <SatelliteAltitude unit="deg">[Satellite_Altitude/Elevation_Angle (90-Zenith)]</SatelliteAltitude>
    <RollViewingAngle unit="deg">[Across_Track_Viewing_Angle]</RollViewingAngle>
    <PitchViewingAngle unit="deg">[Along_Track_Viewing_Angle]</PitchViewingAngle>
    <SunEarthDistance unit="AU">[Sun_Earth_Distance_in_Astronomical_Units]</SunEarthDistance>

    <GeometryCorrectionMethod>[Method_Used e.g., RPC, SensorModel, GCP, None]</GeometryCorrectionMethod>
    <HeightMode>[Source_of_Elevation_Data e.g., DEM, AVE-Height, None]</HeightMode>
    <ResamplingKernel>[Kernel_Used e.g., CC, BL, NN, Raised_Cosine_6P]</ResamplingKernel>
    <RadiometricCalibrationInfo>
        <CalibrationSource>[Source_of_Params e.g., Lab, OnOrbit, Manual]</CalibrationSource>
        <CalibrationDate>[Date_or_Version_of_Params e.g., YYYYMMDD, VersionString]</CalibrationDate>
    </RadiometricCalibrationInfo>
    <MtfCorrection>[Applied/Method e.g., LAB, OnOrbit, Yes/No]</MtfCorrection>
    <DenoiseMethod>[Method_Used e.g., MOMENT, Yes/No, None]</DenoiseMethod>
    <RayleighCorrection>[Applied Yes/No]</RayleighCorrection>
    <GainSettings>[Raw_Gain_Mode_Settings e.g., G2,G1,G1,G2 or 114]</GainSettings> 
    <IntegrationTimeInfo unit="ms or s">[Integration_Time_Setting(s)]</IntegrationTimeInfo> 
    <IntegrationLevelInfo>[Integration_Level_Setting(s) e.g., S5,S4 or 16]</IntegrationLevelInfo> <ProductComponents>
        <ComponentFile>
            <FileType>[Type_Identifier e.g., Metadata, ImageData, RPC, BrowseImage, ThumbImage, QualityReport, CalibrationFile, GeoRange, SpectralParam]</FileType>
            <FileName>[Relative_or_Absolute_FileName]</FileName>
            <Format>[File_Format e.g., XML, GeoTIFF, TXT, JPG, SHP]</Format>
            <Location>[Relative_Path_or_URL]</Location>
        </ComponentFile>
        </ProductComponents>

    <ImageQuality>
        <CloudCoverPercent>[Percentage_Value_0-100]</CloudCoverPercent>
        <CloudShadowPercent>[Percentage_Value_0-100]</CloudShadowPercent>
        <SnowCoverPercent>[Percentage_Value_0-100]</SnowCoverPercent>
        <IceCoverPercent>[Percentage_Value_0-100]</IceCoverPercent>
        <OverallQuality>[Overall_Assessment_Code_or_Description e.g., 4, Good, Degraded]</OverallQuality>
        <RadiometricQualityChecks>
            <CCDErrorCheck status="[Pass/Fail/NotChecked]">[Details_or_Code]</CCDErrorCheck>
            <RandomCodeCheck status="[Pass/Fail/NotChecked]">[Details_or_Code]</RandomCodeCheck>
            <TapAndDeadPixelCheck status="[Pass/Fail/NotChecked]">[Details_or_Code]</TapAndDeadPixelCheck>
            <ImageMissingCheck status="[Pass/Fail/NotChecked]">[Details_or_Code]</ImageMissingCheck>
            <ColorOffsetCheck status="[Pass/Fail/NotChecked]">[Details_or_Code]</ColorOffsetCheck>
            <StripingNoiseCheck status="[Pass/Fail/NotChecked]">[Details_or_Code]</StripingNoiseCheck>
        </RadiometricQualityChecks>
        <GeometricQualityChecks>
            <ReferenceData>[Source_Used_for_Check e.g., DOM_8m, DEM_30m, GCPs]</ReferenceData>
            <CheckPointCount>[Number_of_Check_Points_Used]</CheckPointCount>
            <LinePrecision unit="m">[Precision_in_Line_Direction]</LinePrecision>
            <SamplePrecision unit="m">[Precision_in_Sample_Direction]</SamplePrecision>
            <OverallPrecision unit="m">[Overall_RMSE_or_CE90]</OverallPrecision>
            <CheckTime>[YYYY-MM-DDTHH:MM:SSZ]</CheckTime>
            <CheckUnit>[Unit_Performing_Check]</CheckUnit>
        </GeometricQualityChecks>
        <QualityReportFile>[FileName_or_Path_to_Detailed_Report, if available]</QualityReportFile>
    </ImageQuality>

    <ARDProductMetaData>
        <ARDProductID>[ARD_Product_Identifier]</ARDProductID>
        <ARDProductTime>[YYYY-MM-DDTHH:MM:SSZ]</ARDProductTime>
        <ARDLevel>[ARD_Level e.g., SR, BT, ST]</ARDLevel> <CollectionNumber>[Collection_Identifier]</CollectionNumber>
        <TileNumber>[Tile_Identifier e.g., MGRS]</TileNumber>
        <ARDProduceUnit>[Unit_Producing_ARD]</ARDProduceUnit>
    </ARDProductMetaData>
    
    
</ProductMetaData>